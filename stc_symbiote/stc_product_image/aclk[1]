</script>

    
    
    
    
    
	
	
<script type="text/javascript">
digitalData.page.pageInfo.pageTrack = "product detail";
digitalData.product.model_code = "RR23A2F3X4R\/HL".replace(/&/g, ' and ').replace(/  /g,' ');
digitalData.product.displayName = "225L Digi\u002DTouch Cool™ Single Door Refrigerator RR23A2F3X4R".replace(/(<([^>]+)>)/gi, "").replace(/&/g, ' and ').replace(/  /g,' ');
